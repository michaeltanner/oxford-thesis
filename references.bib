	author = {Gaita, Fiorenzo and Riccardi, Riccardo and Caponi, Domenico and Shah, Dipen and Garberoglio, Lucia and Vivalda, Laura and Dulio, Alessandro and Chiecchio, Andrea and Manasse, Eric and Gallotti, Roberto},
