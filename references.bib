	urldate = {2014-04-28},
	journal = {Heart Rhythm},
	author = {Malcolme-Lawes, L. C. and Juli, C. and Karim, R. and Bai, W. and Quest, R. and Lim, P. B. and Jamil-Copley, S. and Kojodjojo, P. and Ariff, B. and Davies, D. W. and Rueckert, D. and Francis, D. P. and Hunter, R. and Jones, D. and Boubertakh, R. and Petersen, S. E. and Schilling, R. and Kanagaratnam, P. and Peters, N. S.},
