	urldate = {2014-12-24},
	journal = {The Annals of Thoracic Surgery},
	author = {Chiappini, Bruno and Mart{\`i}n-Su{\`a}rez, Sofia and LoForte, Antonino and Arpesella, Giorgio and Di Bartolomeo, Roberto and Marinelli, Giuseppe},
