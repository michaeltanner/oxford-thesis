	pages = {470--476},
	file = {Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/69IWSGCC/Yu et al. - 1999 - Reversal of atrial electrical remodeling following.pdf:application/pdf;Snapshot:/Users/mcmanigle/Documents/Zotero/storage/X6KBSR8N/470.html:text/html}
