	keywords = {Algorithms, Atrial Fibrillation, Benchmarking, Cicatrix, Contrast Media, Databases, Factual, Europe, fibrosis, Heart Atria, Humans, Image Interpretation, Computer-Assisted, Magnetic Resonance Imaging, Observer Variation, Predictive Value of Tests, Reproducibility of Results, United States},
	pages = {105},
