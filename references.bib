	file = {ScienceDirect Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/76BUTRB4/Watson et al. - 2009 - Mechanisms of thrombogenesis in atrial fibrillatio.pdf:application/pdf;ScienceDirect Snapshot:/Users/mcmanigle/Documents/Zotero/storage/BPSB6W5N/S0140673609600404.html:text/html}
}

