	urldate = {2011-06-10},
	journal = {Journal of the American College of Cardiology},
	author = {Ferket, Bart S. and Genders, Tessa S. S. and Colkesen, Ersen B. and Visser, Jacob J. and Spronk, Sandra and Steyerberg, Ewout W. and Hunink, M. G. Myriam},
