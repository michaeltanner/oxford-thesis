	urldate = {2014-09-30},
	journal = {Heart Rhythm},
	author = {Khurram, Irfan M. and Beinart, Roy and Zipunnikov, Vadim and Dewire, Jane and Yarmohammadi, Hirad and Sasaki, Takeshi and Spragg, David D. and Marine, Joseph E. and Berger, Ronald D. and Halperin, Henry R. and Calkins, Hugh and Zimmerman, Stefan L. and Nazarian, Saman},
