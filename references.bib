	pages = {187 --198},
	file = {IEEE Xplore Abstract Record:/Users/mcmanigle/Documents/Zotero/storage/S3ZFPADJ/Maes et al. - 1997 - Multimodality image registration by maximization o.html:text/html;IEEE Xplore Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/G28TRGSW/Maes et al. - 1997 - Multimodality image registration by maximization o.pdf:application/pdf}
