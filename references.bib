	keywords = {ablation, Atrial Fibrillation, atrial flutter, atrial tachycardia, atypical, cardiac ablation, cardiac arrhythmia, coronary sinus structure, left atrial structure, tachycardia},
	pages = {83--91},
	file = {03053.pdf:/Users/mcmanigle/Documents/Zotero/storage/NDA3UEE6/03053.pdf:application/pdf}
}

