	author = {Rajpoot, Kashif and Grau, Vicente and Alison Noble, J. and Becher, Harald and Szmigielski, Cezary},
	month = aug,
	year = {2011},
	keywords = {Left ventricle, Multi-view fusion, Real-time 3D echocardiography, Segmentation, Tracking},
