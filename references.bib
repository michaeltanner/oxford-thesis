	keywords = {Adult, Cardiac Volume, Female, Humans, Image Processing, Computer-Assisted, Imaging, Three-Dimensional, Magnetic Resonance Imaging, Cine, Male, Myocardial Contraction},
	pages = {411--416},
	file = {20145_ftp.pdf:/Users/mcmanigle/Documents/Zotero/storage/6F9D2DN8/20145_ftp.pdf:application/pdf}
