	keywords = {Atrial Fibrillation, cardiology, Catheter Ablation, Cicatrix, Cohort Studies, Contrast Media, Diagnostic Imaging, electrophysiology, gadolinium, Heart Atria, Humans, Image Processing, Computer-Assisted, Kinetics, Magnetic Resonance Imaging, Observer Variation, Reproducibility of Results},
