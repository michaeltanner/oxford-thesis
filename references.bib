	journal = {Radiology},
	author = {Peters, Dana C. and Wylie, John V. and Hauser, Thomas H. and Kissinger, Kraig V. and Botnar, Ren{\'e} M. and Essebag, Vidal and Josephson, Mark E. and Manning, Warren J.},
