	pages = {353--360},
	file = {ScienceDirect Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/A6PGKD86/Reddy et al. - 2008 - Balloon catheter ablation to treat paroxysmal atri.pdf:application/pdf;ScienceDirect Snapshot:/Users/mcmanigle/Documents/Zotero/storage/CG7CGVBD/S1547527107011010.html:text/html}
