	pages = {547--554},
	file = {Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/QXJPJR9Q/Al-Saady et al. - 1999 - Left atrial appendage structure, function, and ro.pdf:application/pdf;Snapshot:/Users/mcmanigle/Documents/Zotero/storage/N8E5V7A8/547.html:text/html}
}

