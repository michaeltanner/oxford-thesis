	keywords = {Atrial Fibrillation, electroanatomic mapping, Late gadolinium{\textendash}enhanced magnetic resonance imaging, scar},
	pages = {85--92},
	file = {ScienceDirect Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/GIZ7UJPH/Khurram et al. - 2014 - Magnetic resonance image intensity ratio, a normal.pdf:application/pdf;ScienceDirect Snapshot:/Users/mcmanigle/Documents/Zotero/storage/7GTTN9S5/S1547527113011442.html:text/html}
