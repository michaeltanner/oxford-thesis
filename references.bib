	file = {Full Text PDF:/Users/mcmanigle/Documents/Zotero/storage/XJW59AR3/Oral et al. - 2006 - Risk of Thromboembolic Events After Percutaneous L.pdf:application/pdf;Snapshot:/Users/mcmanigle/Documents/Zotero/storage/SF5T9TE3/759.html:text/html}
